{
  "name": "obsidian-plugin-workshop",
  "version": "1.0.0",
  "description": "A workshop for developing Obsidian plugins with AI assistance",
  "main": "index.js",
  "scripts": {
    "analyze": "ts-node .workshop-meta/plugin-analyzer.ts",
    "analyze-docs": "ts-node .workshop-meta/docs-analyzer.ts",
    "test": "jest",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "docs:api": "typedoc",
    "docs:templates": "ts-node scripts/generate-docs.ts",
    "docs": "npm run docs:api && npm run docs:templates"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "development",
    "ai",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^19.0.13",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0",
    "jsdom": "^22.1.0",
    "node-fetch": "^3.3.2",
    "obsidian": "latest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.19.6",
    "@types/node-fetch": "^2.6.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1"
  },
  "lint-staged": {
    "*.ts": "npx eslint --cache --fix"
  }
} 